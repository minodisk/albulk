{
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "360000"
  },
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(cat:*)",
      "Bash(chmod:*)",
      "Bash(cp:*)",
      "Bash(echo:*)",
      "Bash(env)",
      "Bash(find:*)",
      "Bash(gh api:*)",
      "Bash(gh issue view:*)",
      "Bash(gh pr checks:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr diff:*)",
      "Bash(gh pr edit:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr view:*)",
      "Bash(gh run list:*)",
      "Bash(gh run view:*)",
      "Bash(gh run watch:*)",
      "Bash(git add:*)",
      "Bash(git blame:*)",
      "Bash(git checkout:*)",
      "Bash(git clone:*)",
      "Bash(git commit:*)",
      "Bash(git config --get:*)",
      "Bash(git describe:*)",
      "Bash(git diff:*)",
      "Bash(git fetch:*)",
      "Bash(git init:*)",
      "Bash(git log:*)",
      "Bash(git ls-files:*)",
      "Bash(git pull:*)",
      "Bash(git push:*)",
      "Bash(git remote:*)",
      "Bash(git rev-parse:*)",
      "Bash(git show:*)",
      "Bash(git status:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(jq:*)",
      "Bash(ln:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(node:*)",
      "Bash(npm init:*)",
      "Bash(npm install:*)",
      "Bash(npm run:*)",
      "Bash(npx:*)",
      "Bash(pnpm add:*)",
      "Bash(pnpm build:*)",
      "Bash(pnpm dev:*)",
      "Bash(pnpm dlx:*)",
      "Bash(pnpm install:*)",
      "Bash(pnpm run:*)",
      "Bash(pwd)",
      "Bash(rm:*)",
      "Bash(sed:*)",
      "Bash(tail:*)",
      "Bash(touch:*)",
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Bash(which:*)",
      "Edit(./**)",
      "Glob(**)",
      "Grep(**)",
      "LS(**)",
      "Read(**)",
      "WebFetch(domain:*)",
      "WebSearch",
      "Write(./**)"
    ],
    "deny": [
      "Edit(//bin/**)",
      "Edit(//etc/**)",
      "Edit(//lib/**)",
      "Edit(//opt/**)",
      "Edit(//proc/**)",
      "Edit(//sbin/**)",
      "Edit(//sys/**)",
      "Edit(//usr/**)",
      "Edit(//var/**)",
      "Write(//bin/**)",
      "Write(//etc/**)",
      "Write(//lib/**)",
      "Write(//opt/**)",
      "Write(//proc/**)",
      "Write(//sbin/**)",
      "Write(//sys/**)",
      "Write(//usr/**)",
      "Write(//var/**)"
    ]
  }
}
